<script is:inline>
const carouselList = document.querySelector('.carousel__list');
const carouselItems = document.querySelectorAll('.carousel__item');
const elems = Array.from(carouselItems);

// Function to update the carousel positions
function update(newActivePos) {
  const current = elems.find((elem) => elem.dataset.pos == 0);
  const prev = elems.find((elem) => elem.dataset.pos == -1);
  const next = elems.find((elem) => elem.dataset.pos == 1);
  const first = elems.find((elem) => elem.dataset.pos == -2);
  const last = elems.find((elem) => elem.dataset.pos == 2);
  
  console.log("current:", current);
  console.log("prev:", prev);
  console.log("next:", next);
  console.log("first:", first);
  console.log("last:", last);

  current.classList.remove('carousel__item_active');
  
  [current, prev, next, first, last].forEach(item => {
    let itemPos = item.dataset.pos;

    item.dataset.pos = getPos(itemPos, newActivePos);
  });
}


// Function to calculate new position
function getPos(current, active) {
  const diff = current - active;

  if (Math.abs(diff) > 2) {
    return -current;
  }

  return diff;
}

// Function to start the carousel
function startCarousel() {
  let currentIndex = 0; // Start with the first item
  const intervalId = setInterval(() => {
    const nextIndex = (currentIndex + 1) % elems.length; // Calculate the index of the next item
    update(elems[nextIndex].dataset.pos);
    currentIndex = nextIndex; // Update currentIndex
  }, 20000); // Adjust the interval time (in milliseconds) as needed

  return intervalId; // Return the intervalId for later use (e.g., stopping the carousel)
}

// Start the carousel
const carouselInterval = startCarousel();

// Event listener for clicking on carousel items
carouselList.addEventListener('click', function(event) {
  const newActive = event.target;
  const isItem = newActive.closest('.carousel__item');

  if (isItem && !newActive.classList.contains('carousel__item_active')) {
    update(isItem.dataset.pos);
  }
});


</script>



